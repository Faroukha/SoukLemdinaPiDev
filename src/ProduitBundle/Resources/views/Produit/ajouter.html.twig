{% extends 'layout/layout.html.twig' %}
{% block body %}

    <div class="col-md-12 col-sm-6 col-xs-12">
        <form action="{{ path("Ajouter_Produit") }}" method="POST">


            <div class="col-md-5">

                <div class="form-group">
                    <label>Categorie</label>
                    <input   id="categorie" name="categorie" type="text" class="form-control">
                </div>
                <div class="form-group">
                    <label>Titre</label>
                    <input  id="titre" name="titre" type="text"  class="form-control">
                </div>
                <div class="form-group">
                    <label>Description</label>
                    <input  id="description" name="description" type="text"  class="form-control">
                </div>
                <div class="form-group">
                    <label>Prix</label>
                    <input  id="prix" name="prix" type="text"  class="form-control">
                </div>

                <div class="form-group">
                    <label>Image</label>
                  <input type="file" name="image">

                </div>
                <div class="form-group">
                    <label>Quantite</label>
                    <input  id="quantite" name="quantite" type="text"  class="form-control">
                </div>
            </div>
                 <input type="submit" name="Submit">
         </form>
    </div>




{% endblock %}